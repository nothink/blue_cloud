{
  "name": "blue_cloud",
  "version": "0.0.1",
  "private": true,
  "description": "Rei Shinomiya",
  "main": "main.js",
  "repository": "git@github.com:nothink/blue_cloud.git",
  "author": "nothink <nothing@yasagure.jp>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "yarn clean && yarn lint && yarn compile",
    "clean": "rm -rf dst",
    "compile": "tsc -p .",
    "lint": "eslint 'src/*.ts' 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/*.ts' 'src/**/*.ts'",
    "eslint-check": "eslint --print-config src/main.ts | eslint-config-prettier-check"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "commander": "^6.0.0",
    "config": "^3.3.1",
    "moment": "^2.24.0",
    "puppeteer-core": "^5.2.1",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/config": "^0.0.36",
    "@types/node": "^14.0.26",
    "@types/puppeteer-core": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tsutils": "^3.17.1",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
